{
    "resourceSpecId": "3f2c6e74-0b8a-4a6f-9a5e-7c5e2e2a1b11",
    "stableId": "main-redis",
    "displayName": "Main Redis Cluster",
    "category": "database",
    "iconUrl": "https://cdn.sigmoid.us/icons/aws/full-size/svg/database/elasticache.svg",
    "resourceType": "cache.elasticache",
    "vpcDependency": "required",
    "status": "draft",
    "position": { "x": 500, "y": 300 },
    "terraform": {
        "module": {
            "source": "github.com/sigmoid-hq/terraform-aws-elasticache",
            "ref": "main"
        }
    },
    "pricing": {
        "awsPricingLookup": {
            "serviceCode": "AmazonElastiCache",
            "unit": "hourly",
            "quantityProperties": ["num_cache_nodes", "replica_count"],
            "quantityDefault": 1,
            "filters": [
                { "field": "location", "value": "${location}" },
                { "field": "instanceType", "propertyKeys": ["node_type", "instance_type"] },
                { "field": "cacheEngine", "propertyKey": "engine", "defaultValue": "Redis", "required": false }
            ]
        },
        "usageProfiles": {
            "default": "medium",
            "low": {
                "num_cache_nodes": 1,
                "replica_count": 1,
                "count": 1
            },
            "medium": {
                "num_cache_nodes": 2,
                "replica_count": 2,
                "count": 2
            },
            "high": {
                "num_cache_nodes": 4,
                "replica_count": 4,
                "count": 4
            },
            "xhigh": {
                "num_cache_nodes": 8,
                "replica_count": 8,
                "count": 8
            }
        }
    },
    "easy": {
        "alias": "Database",
        "group": "data-database",
        "priority": 40,
        "allowedProperties": ["engine", "node_type", "num_cache_nodes", "replica_count", "count"]
    }
}
